              void GUI_ShowPicture(uchar x, uint y, uchar wide, uint high)
              {
                      uint temp = 0, tmp = 0, num = 0;
                      TFT_SetWindow(x, y, x+wide-1, y+high-1);
                      num = wide * high * 2;
                      do
                      {  
                              temp = pic[tmp + 1];
                              temp = temp << 8;
                              temp = temp | pic[tmp];
